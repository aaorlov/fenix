{
  "compilerOptions": {
      "module": "commonjs",
      "target": "es5",
      "noImplicitAny": false,
      "sourceMap": false,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "removeComments": true,
      "noEmitOnError": false,
      "typeRoots": ["node_modules/@types"],
      "types": [
          "node",
          "lodash",
          "xml"
      ],
      "outDir": "dist"
  }
}